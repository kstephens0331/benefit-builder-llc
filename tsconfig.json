{
    "compilerOptions":  {
                            "target":  "ES2020",
                            "useDefineForClassFields":  true,
                            "lib":  [
                                        "ES2020",
                                        "DOM",
                                        "DOM.Iterable"
                                    ],
                            "module":  "ESNext",
                            "skipLibCheck":  true,
                            "jsx":  "react-jsx",
                            "moduleResolution":  "Bundler",
                            "resolveJsonModule":  true,
                            "isolatedModules":  true,
                            "noEmit":  true,
                            "types": ["vite/client","@cloudflare/workers-types",
                                          "node",
                                          "vite/client",
                                      ],
                            "strict":  true,
                            "baseUrl":  "."
                        },
    "include":  [
                    "src", "vite.config.ts", "functions", "src/vite-env.d.ts"
                ]
}

